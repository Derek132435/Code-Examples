all: part1 part2

part1: part1.o HashMap.o
	g++ part1.o HashMap.o -std=c++11 -o part1

part2: part2.o HashMap.o
	g++ part2.o HashMap.o -std=c++11 -o part2

part1.o: part1.cpp
	g++ -std=c++11 -c part1.cpp

part2.o: part2.cpp
	g++ -std=c++11 -c part2.cpp

HashMap.o: HashMap.cpp
	g++ -std=c++11 -c HashMap.cpp

clean:
	del part1.exe part2.exe
